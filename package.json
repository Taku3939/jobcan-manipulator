{
  "name": "jobcan-manipulator",
  "version": "0.0.1",
  "description": "ジョブカンの自動操作",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "projects": "ts-node -r tsconfig-paths/register ./src/commands/index.ts projects",
    "complete": "ts-node -r tsconfig-paths/register ./src/commands/complete.ts complete",
    "build-js": "esbuild ./src/index.ts --bundle --outfile=./dist/out.js --platform=node",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Takuya Isaki",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "nqdm": "^2.0.2",
    "playwright": "^1.38.0"
  },
  "devDependencies": {
    "esbuild": "0.19.3",
    "rome": "^12.1.3",
    "ts-loader": "^9.4.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
